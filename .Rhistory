data %>%
mutate({{ var_name }} :=
case_when(
{{col}} == 1 ~ nep_months[1],
{{col}} == 2 ~ nep_months[2],
{{col}} == 3 ~ nep_months[3],
{{col}} == 4 ~ nep_months[4],
{{col}} == 5 ~ nep_months[5],
{{col}} == 6 ~ nep_months[6],
{{col}} == 7 ~ nep_months[7],
